        "test": "npm run compile && node ./node_modules/vscode/bin/test",
        "tslint": "tslint -c tslint.json 'src/**/*.ts'"
    },
    "devDependencies": {
        "typescript": "^2.5.2",
        "vscode": "^1.1.5",
        "mocha": "^3.5.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42",
        "typemoq": "^2.0.1",
        "@types/glob": "^5.0.32",
